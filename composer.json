{
    "name": "iagapie/php-auth",
    "description": "Authentication library inspired by the Symfony Security",
    "keywords": [
        "authentication",
        "auth",
        "security"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Agapie",
            "email": "igoragapie@gmail.com"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/iagapie/security-csrf"
    }],
    "require": {
        "php": ">= 8.0",
        "ext-json": "*",
        "ext-sodium": "*",
        "iagapie/cookie-jar": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "^1.1",
        "symfony/event-dispatcher": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/security-csrf": "5.x-dev"
    },
    "require-dev": {
        "ext-pdo": "*",
        "psr/http-server-middleware": "^1.0",
        "symfony/console": "^5.2",
        "doctrine/dbal": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IA\\Auth\\": "src/"
        }
    }
}
